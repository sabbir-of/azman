<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400">
  <defs>
    <linearGradient id="p1Grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2EAD33"/>
      <stop offset="50%" style="stop-color:#D65348"/>
      <stop offset="100%" style="stop-color:#E5A03D"/>
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="#0f172a"/>
  <rect x="40" y="40" width="520" height="280" rx="10" fill="#1e293b"/>
  <circle cx="80" cy="70" r="10" fill="#ef4444"/>
  <circle cx="110" cy="70" r="10" fill="#f59e0b"/>
  <circle cx="140" cy="70" r="10" fill="#22c55e"/>
  <rect x="60" y="100" width="480" height="200" rx="5" fill="#0f172a"/>
  <text x="80" y="140" font-family="monospace" font-size="14" fill="#22c55e">import { test, expect } from '@playwright/test';</text>
  <text x="80" y="170" font-family="monospace" font-size="14" fill="#94a3b8">test('login flow', async ({ page }) => {</text>
  <text x="100" y="200" font-family="monospace" font-size="14" fill="#0ea5e9">await page.goto('/login');</text>
  <text x="100" y="230" font-family="monospace" font-size="14" fill="#0ea5e9">await page.fill('#email', 'test@test.com');</text>
  <text x="100" y="260" font-family="monospace" font-size="14" fill="#14b8a6">await expect(page).toHaveURL('/dashboard');</text>
  <text x="80" y="290" font-family="monospace" font-size="14" fill="#94a3b8">});</text>
  <text x="300" y="360" font-family="Arial" font-size="18" fill="url(#p1Grad)" text-anchor="middle" font-weight="bold">Playwright TypeScript Framework</text>
</svg>
